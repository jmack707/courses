{
    "class": "ADC",
    "schemaVersion": "3.6.0",
    "id": "AS3-APPS-DC1",
   
    "Common": {
        "class": "Tenant",
        "Shared": {
            "class": "Application",
            "template": "shared",
            "DC1": {
                "class": "GSLB_Data_Center"
            },
            "bigip1.f5lab.local": {
                "class": "GSLB_Server",
                "dataCenter": {
                    "use": "DC1"
                },
                "devices": [
                    {
                        "address": "10.1.20.4"
                    }
                ],
                "virtualServers": [
                    {
                        "address": "10.1.10.100",
                        "port": 443,
                        "name": "dc1.app1.https.vs"
                    }
                ]                
            },              
            "bigip5.f5lab.local": {
                "class": "GSLB_Server",
                "dataCenter": {
                    "use": "DC1"
                },
                "devices": [
                    {
                        "address": "10.1.20.11"
                    }
                ]
            },
            "DC2": {
                "class": "GSLB_Data_Center"
            },
            "bigip2.f5lab.local": {
                "class": "GSLB_Server",
                "dataCenter": {
                    "use": "DC2"
                },  
                "devices": [
                    {
                        "address": "10.1.20.5"
                    }                    
                ]                           
            },             
            "bigip6.f5lab.local": {
                "class": "GSLB_Server",
                "dataCenter": {
                    "use": "DC2"
                },
                "devices": [
                    {
                        "address": "10.1.20.12"
                    }
                ]
                              
            }
        },
        "APP1_Tenant1": {
            "class": "Tenant",
            "Application": {
                "class": "Application",
                "template": "shared",
                "app1.gslb.acme.com": {
                    "class": "GSLB_Domain",
                    "domainName": "app1.gslb.acme.com",
                    "resourceRecordType": "A",
                    "lastResortPool": {
                        "use": "app1.https.gtm.pl"
                    },
                    "lastResortPoolType": "A",
                    "pools": [
                        {
                            "use": "app1.https.gtm.pl"
                        }                  
    
                    ]
                },
                "app1.https.gtm.pl": {
                    "class": "GSLB_Pool",
                    "resourceRecordType": "A",
                    "members": [
                        {
                            "server": {
                                "use": "/Common/Shared/bigip1.f5lab.local"
                            },
                            "virtualServer": "dc1.app1.https.vs"
                        }
    
                    ]
                }          
            }
        }
    }
    
}