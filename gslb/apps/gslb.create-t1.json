{
    "class": "ADC",
    "schemaVersion": "3.6.0",
    "id": "GSLB_Sample",

    "Example_Tenant1": {
        "class": "Tenant",
        "Application": {
            "class": "Application",
            "template": "shared",
            "testDomain": {
                "class": "GSLB_Domain",
                "domainName": "app1.gslb.acme.com",
                "resourceRecordType": "A",
                "pools": [
                    {
                        "use": "app1.https.gtm.pl"
                    }                  

                ]
            },
            "app1.https.gtm.pl": {
                "class": "GSLB_Pool",
                "resourceRecordType": "A",
                "members": [
                    {
                        "server": {
                            "use": "/Common/Shared/bigip1.f5lab.local"
                        },
                        "virtualServer": "dc1.app1.https.vs"
                    }

                ]
            }          
        }
    },

    
    "Common": {
        "class": "Tenant",
        "Shared": {
            "class": "Application",
            "template": "shared",
            "dc1": {
                "class": "GSLB_Data_Center"
            },
            "bigip1.f5lab.local": {
                "class": "GSLB_Server",
                "dataCenter": {
                    "use": "dc1"
                },
                "devices": [
                    {
                        "address": "10.1.20.4"
                    }
                ],
                "virtualServers": [
                        {
                            "address": "10.1.10.100",
                            "port": 443,
                            "name": "dc1.app1.https.vs"
                        }
                    
                    ]
            }   
        }
    }
}