{
    "class": "ADC",
    "schemaVersion": "3.6.0",
    "id": "GSLB_Sample",

    "Example_Tenant": {
        "class": "Tenant",
        "Application": {
            "class": "Application",
            "template": "shared",
            "testDomain": {
                "class": "GSLB_Domain",
                "domainName": "app1.gslb.acme.com",
                "resourceRecordType": "A",
                "pools": [
                    {
                        "use": "app1.https.gtm.pl"
                    }                  

                ]
            },
            "testDomain2": {
                "class": "GSLB_Domain",
                "domainName": "app2.gslb.acme.com",
                "resourceRecordType": "A",
                "pools": [
                    {
                        "use": "app2.https.gtm.pl"
                    }                    

                ]
            },            
            "app1.https.gtm.pl": {
                "class": "GSLB_Pool",
                "resourceRecordType": "A",
                "members": [
                    {
                        "server": {
                            "use": "/Common/Shared/bigip1.f5lab.local"
                        },
                        "virtualServer": "dc1.app1.https.vs"
                    },
                                        {
                        "server": {
                            "use": "/Common/Shared/bigip2.f5lab.local"
                        },
                        "virtualServer": "dc2.app1.https.vs"
                    }

                ]
            },
            "app2.https.gtm.pl": {
                "class": "GSLB_Pool",
                "resourceRecordType": "A",
                "members": [
                    {
                        "ratio": 10,
                        "server": {
                            "use": "/Common/Shared/bigip1.f5lab.local"
                        },
                        "virtualServer": "dc1.app2.https.vs"
                    }

                ]
            }            
        }
    },

    
    "Common": {
        "class": "Tenant",
        "Shared": {
            "class": "Application",
            "template": "shared",
            "gtm.dc1": {
                "class": "GSLB_Data_Center"
            },
            "bigip5.f5lab.local": {
                "class": "GSLB_Server",
                "dataCenter": {
                    "use": "gtm.dc1"
                },
                "devices": [
                    {
                        "address": "10.1.20.11"
                    }
                ]
            },
            "gtm.dc2": {
                "class": "GSLB_Data_Center"
            },
            "bigip6.f5lab.local": {
                "class": "GSLB_Server",
                "dataCenter": {
                    "use": "gtm.dc2"
                },
                "devices": [
                    {
                        "address": "10.1.20.12"
                    }
                ]                
            },
            "ltm.dc1": {
                "class": "GSLB_Data_Center"
            },
            "bigip1.f5lab.local": {
                "class": "GSLB_Server",
                "dataCenter": {
                    "use": "ltm.dc1"
                },
                "devices": [
                    {
                        "address": "10.1.20.4"
                    }
                ],
                "virtualServers": [
                        {
                            "address": "10.1.10.100",
                            "port": 443,
                            "name": "dc1.app1.https.vs"
                        },
                    {
                        "address": "10.1.10.101",
                        "port": 443,
                        "name": "dc1.app2.https.vs"
                    }                        
                    ]
            },
            "ltm.dc2": {
                "class": "GSLB_Data_Center"
            },
            "bigip2.f5lab.local": {
                "class": "GSLB_Server",
                "dataCenter": {
                    "use": "ltm.dc2"
                },  
                "devices": [
                    {
                        "address": "10.1.20.5"
                    }                    
                ],
                "virtualServers": [
                    {
                        "address": "10.1.10.102",
                        "port": 443,
                        "name": "dc2.app1.https.vs"
                    },
                    {
                        "address": "10.1.10.103",
                        "port": 443,
                        "name": "dc2.app2.https.vs"
                    }                    
                ]                             

            }            
        }
    }
}