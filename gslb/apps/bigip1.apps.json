{
    "class": "AS3",
    "action": "deploy",
    "persist": true,
    "declaration": {
      "class": "ADC",
      "schemaVersion": "3.0.0",
      "id": "gslbdemo1",
      "label": "BIGIP1 Demo App",
      "remark": "Demo Applications on BIGIP1",
      "controls": {
        "trace": true
      },
      "GSLB": {
        "class": "Tenant",
        "A1": {
          "class": "Application",
          "dc1.app1.https.vs": {
            "class": "Service_TCP",
            "virtualAddresses": [
              "10.1.10.100"
            ],
            "virtualPort": 443,
            "pool": "app1.https.pl"
          },
          "app1.https.pl": {
            "class": "Pool",
            "monitors": [
              "https"
            ],
            "members": [{
              "servicePort": 6100,
              "serverAddresses": [
                "10.1.20.9"
              ]
            }]
          }
        },
        "A2": {
          "class": "Application",
          "dc1.app2.https.vs": {
            "class": "Service_TCP",
            "virtualAddresses": [
              "10.1.10.101"
            ],
            "virtualPort": 443,
            "pool": "app2.https.pl"
          },
          "app2.https.pl": {
            "class": "Pool",
            "monitors": [
              "https"
            ],
            "members": [{
              "servicePort": 6101,
              "serverAddresses": [
                "10.1.20.9"
              ]
            }]
          }
        }
      }
    }
}